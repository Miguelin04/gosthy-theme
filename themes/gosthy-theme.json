{
  "name": "Gosthy",
  "type": "dark",
  "colors": {
    "editor.background": "#0f1419",
    "editor.foreground": "#c9d1d9",
    "editor.selectionBackground": "#2d5aa0",
    "editor.lineHighlightBackground": "#161b22",
    "editor.selectionHighlightBackground": "#3fb95040",
    "sideBar.background": "#0a0e14",
    "sideBar.foreground": "#9bb3d4",
    "sideBar.border": "#1e3a5f",
    "activityBar.background": "#0a0e14",
    "activityBar.foreground": "#6bb6ff",
    "activityBar.border": "#1e3a5f",
    "activityBar.activeBorder": "#6bb6ff",
    "titleBar.activeBackground": "#010409",
    "titleBar.activeForeground": "#c9d1d9",
    "titleBar.border": "#21262d",
    "tab.activeBackground": "#0f1419",
    "tab.activeForeground": "#e1e8f0",
    "tab.inactiveBackground": "#0a0e14",
    "tab.inactiveForeground": "#9bb3d4",
    "tab.border": "#1e3a5f",
    "panel.background": "#010409",
    "panel.border": "#21262d",
    "terminal.background": "#0d1117",
    "terminal.foreground": "#ffffff",
    "terminal.ansiBlue": "#6bb6ff",
    "terminal.ansiCyan": "#7fffd4",
    "terminal.ansiGreen": "#90ee90",
    "terminal.ansiYellow": "#ffff00",
    "terminal.ansiRed": "#ff6b6b",
    "terminal.ansiMagenta": "#dda0dd",
    "terminal.ansiWhite": "#ffffff",
    "terminal.ansiBlack": "#2f4f4f",
    "terminal.ansiBrightBlue": "#87ceeb",
    "terminal.ansiBrightCyan": "#7fffd4",
    "terminal.ansiBrightGreen": "#90ee90",
    "terminal.ansiBrightYellow": "#ffff00",
    "terminal.ansiBrightRed": "#ff6b6b",
    "terminal.ansiBrightMagenta": "#dda0dd",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiBrightBlack": "#808080",
    "button.background": "#238636",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#2ea043",
    "input.background": "#0d1117",
    "input.border": "#30363d",
    "input.foreground": "#c9d1d9",
    "list.activeSelectionBackground": "#264f78",
    "list.hoverBackground": "#161b2240",
    "list.focusBackground": "#264f78",
    "statusBar.background": "#1a2332",
    "statusBar.foreground": "#e1e8f0",
    "statusBar.border": "#1e3a5f"
  },
  "tokenColors": [
    {
      "name": "Comentarios",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8b949e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Palabras clave",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#4a9eff"
      }
    },
    {
      "name": "Strings - Valores JSON",
      "scope": [
        "string.quoted.double.json",
        "string.quoted.single.json",
        "meta.structure.dictionary.value.json string"
      ],
      "settings": {
        "foreground": "#6bb6ff"
      }
    },
    {
      "name": "Otros Strings",
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#87ceeb"
      }
    },
    {
      "name": "Números",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "Funciones",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Clases y tipos",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Constantes",
      "scope": [
        "constant",
        "constant.language"
      ],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "CSS Values",
      "scope": [
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#a5d6ff"
      }
    },
    {
      "name": "JavaScript/TypeScript",
      "scope": [
        "variable.other.object.js",
        "variable.other.object.ts"
      ],
      "settings": {
        "foreground": "#ffa657"
      }
    },
    {
      "name": "Corchetes y Paréntesis - Amarillo",
      "scope": [
        "punctuation.section.brackets.begin",
        "punctuation.section.brackets.end",
        "punctuation.section.parens.begin",
        "punctuation.section.parens.end"
      ],
      "settings": {
        "foreground": "#ffff00"
      }
    },
    {
      "name": "Llaves - Rosa",
      "scope": [
        "punctuation.section.braces.begin",
        "punctuation.section.braces.end",
        "punctuation.definition.block"
      ],
      "settings": {
        "foreground": "#ff69b4"
      }
    },
    {
      "name": "Otros delimitadores - Verde",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition.array"
      ],
      "settings": {
        "foreground": "#90ee90"
      }
    },
    {
      "name": "Propiedades JSON - Blanco",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json"
      ],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Valores JSON - Azul Bonito",
      "scope": [
        "meta.structure.dictionary.value.json string.quoted.double",
        "string.quoted.double.json:not(meta.structure.dictionary.key.json)"
      ],
      "settings": {
        "foreground": "#6bb6ff"
      }
    },
    {
      "name": "Rutas de archivos - Azulito",
      "scope": [
        "string.quoted.single",
        "string.quoted.double",
        "meta.path"
      ],
      "settings": {
        "foreground": "#6bb6ff"
      }
    },
    {
      "name": "Nombres de propiedades - Blanco",
      "scope": [
        "variable.other.property",
        "meta.object-literal.key",
        "entity.name.tag.yaml",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#ffffff",
        "fontStyle": "bold"
      }
    }
  ]
}